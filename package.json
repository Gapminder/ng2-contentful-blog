{
  "name": "ng2-contentful-blog",
  "version": "0.0.1-1",
  "scripts": {
    "flow.install:typings": "./node_modules/.bin/typings install",
    "flow.compile": "npm run flow.install:typings && npm run flow.compile:ts && npm run flow.compile:stylus",
    "flow.compile:ts": "./node_modules/.bin/tsc",
    "flow.compile:stylus": "gulp stylus",
    "clean": "rimraf typings build '{components,demo}/**/*.{d.ts,js.map,js,css}' 'index.{js,js.map,d.ts}'",
    "prepublish": "npm run clean && npm run flow.install:typings && npm run flow.compile",
    "dev": "npm run flow.compile:stylus && webpack-dev-server --inline --hot && npm run tslint",
    "build": "./node_modules/.bin/webpack --progress --color",
    "tslint": "node_modules/.bin/gulp tslint"
  },
  "author": "Valor Software",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:Gapminder/ng2-contentful-blog.git"
  },
  "bugs": {
    "url": "https://github.com/Gapminder/ng2-contentful-blog/issues"
  },
