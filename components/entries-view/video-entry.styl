.wrap-block
  margin 0 -50px
  padding 20px 50px
  background-repeat no-repeat
  background-size cover

.video-wrapper
  position relative
  padding-top 56.3%

  iframe,
  embed
    position absolute
    top 0
    left 0
    width 100%
    height 100%

.loader
  position absolute
  top 41%
  left 48%
  border 4px solid #f0f2f4
  border-radius 50%
  border-top 4px solid #9faeb8
  width 37px
  height 37px
  -webkit-animation spin 1s linear infinite
  animation spin 1s linear infinite
